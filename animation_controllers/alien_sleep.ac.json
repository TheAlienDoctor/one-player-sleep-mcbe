{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.alien.sleep": {
			"initial_state": "awake",
			"states": {
				"awake": {
					"transitions": [
						{
							"sleeping": "query.is_sleeping"
						}
					]
				},
				"sleeping": {
					"animations": [
						"a_alien_sleep"
					],
					"transitions": [
						{
							"awake": "!query.is_sleeping"
						}
					]
				}
			}
		}
	}
}